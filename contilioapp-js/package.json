{
  "name": "contilioapp-js",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@canvasjs/react-charts": "^1.0.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "bootstrap": "^5.3.1",
    "formik": "^2.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "compile:sass": "node-sass src/sass/main.scss src/css/style.min.css",
    "watch:sass": "node-sass src/sass/main.scss src/css/style.min.css -w --output-style compressed",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' src/css/style.min.css -o src/css/style.prefix.css",
    "compress:css": "node-sass src/css/style.prefix.css src/css/style.min.css --output-style compressed",
    "start": "npm-run-all --parallel reactstart watch:sass",
    "reactstart": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.15",
    "node-sass": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.28"
  }
}
